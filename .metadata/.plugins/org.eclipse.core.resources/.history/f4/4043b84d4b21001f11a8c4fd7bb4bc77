package kosa.oop3;

public class Person {

	private String name;
	private Role role;
	

	public Person() {
		
	}
	
	
	public Person(String name) {
		super();
		this.name = name;
		
	}


	public String getName() {
		return name;
	}


	public void setName(String name) {
		this.name = name;
	}
	
	
	
	
}
