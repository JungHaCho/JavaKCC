package kosa.oop5;

interface AA{
	BB abc(); // B타입 리턴하려면 B가 있어야함
}

class BB{
	BB(){
		System.out.println(" B 생성자");
	}
}

public class Main3 {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		A a1 = new A() {
			
			@Override
			public BB abc() {
				// TODO Auto-generated method stub
				return new BB();
			}
		}; 
			
		a1.abc();
		
		
	}
}
