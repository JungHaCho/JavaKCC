package kosa.oop2;

public class MyService {

	private OracleDao dao;
	private MySQLDao mydao;
	
	
	public MyService() {
		
	}
	

	public MyService(OracleDao dao) {
		super();
		this.dao = dao;
	}
	
	public MyService(MySQLDao mydao) {
		super();
		this.mydao = mydao;
	}
	
	public void insertService() {
		dao.insert();
	}
	
	public void myinsertService() {
		mydao.insert();
	}

	public MySQLDao getMydao() {
		return mydao;
	}


	public void setMydao(MySQLDao mydao) {
		this.mydao = mydao;
	}


	public OracleDao getDao() {
		return dao;
	}

	public void setDao(OracleDao dao) {
		this.dao = dao;
	}
	
	
}
