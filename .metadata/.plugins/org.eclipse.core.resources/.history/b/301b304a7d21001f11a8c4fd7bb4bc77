package kosa.oop5;

interface A{
	B abc(); // B타입 리턴하려면 B가 있어야함
}

class B{
	B(){
		System.out.println(" B 생성자");
	}
}

public class Main3 {
	
	
	public static void main(String[] args) {
		// TODO Auto-generated method stub
	
		A a1 = new A() {
			
			@Override
			public B abc() {
				// TODO Auto-generated method stub
				return new B();
			}
		}; 
			
		
	}
}
