package kosa.date;

public class ListExam {

	public static void main(String[] args) {

		int arr[] = new int[6];
		int num = (int) Math.random();

		for (int i = 0; i < arr.length; i++) {
			if (i == 0) {
				arr[0] = num;
			} else {
				if (num == arr[i]) {
					arr[i] = (int) Math.random();
				}
			}
			
		}

	}
}
